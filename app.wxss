/* 旋转动画 */
.loading {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99999;
}

.loading .load-wrap {
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.5;
  position: relative;
}

.loading .load-icon {
  width: 85rpx;
  height: 85rpx;
  display: block;
  position: absolute;
  background: url("data:image/png;base64,");
  background-size: 100%;
  left: 50%;
  margin-left: -50rpx;
  top: 50%;
  margin-top: -50rpx;
}

.loading .load-process {
  display: block;
  width: 85rpx;
  height: 85rpx;
  position: absolute;
  background: url("data:image/png;base64,");
   background-size: 100%;
  left: 50%;
  margin-left: -50rpx;
  top: 50%;
  margin-top: -50rpx;
  animation: rotate 1s linear infinite; 
}

@keyframes rotate {
  0% {
    transform: rotate(0deg);
  }

  50% {
    transform: rotate(180deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

@keyframes fadeIn {
  0% {
    opacity: 0; /*初始状态 透明度为0*/
  }

  50% {
    opacity: 0.5; /*中间状态 透明度为0*/
  }

  100% {
    opacity: 1; /*结尾状态 透明度为1*/
  }
}

@keyframes fadeOut {
  0% {
    opacity: 1; /*初始状态 透明度为0*/
  }

  50% {
    opacity: 0.5; /*中间状态 透明度为0*/
  }

  100% {
    opacity: 0; /*结尾状态 透明度为1*/
  }
}


